<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <Description>Redistribution of PBKDF2.NET by therealmagicmike rebuilt for DotNet Core 3.0</Description>
    <Copyright>Copyright (c) 2013 Michael Johnson</Copyright>
    <PackageLicenseFile>License.txt</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/wwestlake/PBKDF2.NETCore</PackageProjectUrl>
    <RepositoryUrl>https://github.com/wwestlake/PBKDF2.NETCore</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>password, hash, crypto</PackageTags>
    <PackageReleaseNotes>Rebuild of PBKDF2.Net for DotNet Core 3.0</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="3.0.0" />
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="4.6.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\License.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
